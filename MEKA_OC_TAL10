section .data
char db 0
newline db 10

section .text
global _start

_start:
mov al, 00000011b
rol al, 4
shr al, 1
shl al, 2
ror al, 3
add al, 29

; Guardar en char
mov [char], al

; Escribir carácter en consola
mov eax, 4 ; syscall write
mov ebx, 1 ; stdout
mov ecx, char
mov edx, 1 ; 1 byte
int 0x80

; Salto de línea
mov eax, 4
mov ebx, 1
mov ecx, newline
mov edx, 1
int 0x80

; Salir
mov eax, 1 ; syscall exit
xor ebx, ebx ; código de salida 0
int 0x80
